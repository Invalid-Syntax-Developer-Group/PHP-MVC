{
    "name": "isdv/php-mvc",
    "description": "A simple PHP MVC framework.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpMvc\\": "src/",
            "PhpMvc\\Cache\\": "src/Cache/",
            "PhpMvc\\Cache\\Driver\\": "src/Cache/Driver/",
            "PhpMvc\\Cache\\Exception\\": "src/Cache/Exception/",
            "PhpMvc\\Database\\": "src/Database/",
            "PhpMvc\\Email\\": "src/Email/",
            "PhpMvc\\FileSystem\\": "src/FileSystem/",
            "PhpMvc\\Http\\": "src/Http/",
            "PhpMvc\\Logging\\": "src/Logging/",
            "PhpMvc\\Logging\\Driver\\": "src/Logging/Driver/",
            "PhpMvc\\Logging\\Exception\\": "src/Logging/Exception/",
            "PhpMvc\\Provider\\": "src/Provider/",
            "PhpMvc\\Queue\\": "src/Queue/",
            "PhpMvc\\Routing\\": "src/Routing/",
            "PhpMvc\\Routing\\Exception\\": "src/Routing/Exception/",
            "PhpMvc\\Session\\": "src/Session/",
            "PhpMvc\\Session\\Driver\\": "src/Session/Driver/",
            "PhpMvc\\Session\\Exception\\": "src/Session/Exception/",
            "PhpMvc\\Support\\": "src/Support/",
            "PhpMvc\\Validation\\": "src/Validation/",
            "PhpMvc\\Validation\\Exception\\": "src/Validation/Exception/",
            "PhpMvc\\Validation\\Rule\\": "src/Validation/Rule/",
            "PhpMvc\\View\\": "src/View/",
            "PhpMvc\\View\\Engine\\": "src/View/Engine/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "authors": [
        {
            "name": "William L. Holt"
        }
    ],
    "require": {
        "league/flysystem": "^2.0",
        "opis/closure": "^3.6",
        "monolog/monolog": "^2.2",
        "phpmailer/phpmailer": "^6.5"
    }
}
